# -*- coding: utf-8 -*-
def power(x, n):
    if not isinstance(x, (int,float)):
        raise TypeError('x must be a number')
    if not isinstance(n, (int,float)):
        raise TypeError('n must be a number')
    

    s = 1
    while n > 0:
        n = n - 1
        s = s * x
    return s 
root = input('root of the number: ')
po = input('power of the number: ')
a = power(root, po)
print(a)
print('123')